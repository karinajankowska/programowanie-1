#include <stdio.h>
int sum(int tab, int rozmiar){
    int max = tab[0];
    int i;
    for(i=0; i<rozmiar; i++){
        if(max < tab[i]){
        max = tab[i];
    }
}
return max;
}

int sum(int tab, int rozmiar){
    int min = tab[0];
    int i;
    for(i=0; i<rozmiar; i++){
        if(min > tab[i]){
        min = tab[i];
    }
}
return min;
}

int sum(int tab, int rozmiar){
    int suma = 0;
    int i;
    for(i=0; i<6; i++){
        suma += tab[i];
    }
return suma;
    
}

int main()
{
   int tab [6] = {2, 4, 6, 8, 10, 12}
   int i;
   int range = 300;
   int rozmiar = sizeof(tab)/sizeof(tab[0]);
   for(i=0; i<6; i++){
       tab [i] = rand () % range + 1;
       tab [i] = rand () % range - 1;
   }
   for(i=0; i<rozmiar; i++){
      printf("%d\n", tab[i]);
   }
   printf("NajwiÄ™kszy element to %d\n", max(tab, rozmiar));
   printf("Najmniejszy element to %d\n", min(tab, rozmiar));
   printf("Suma elementÃ³w w tablicy to %d", sum(tab, rozmiar));

    return 0;
}
