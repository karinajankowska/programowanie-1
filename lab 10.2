#include <stdio.h>

int main()
{
    int tab[10];
    int i;
    int size = sizeof(tab)/sizeof(int);
    for(i = 0; i < size; i++){
        scanf("%d", &tab[i]);
    }
    printf("Wpisz 0 w kolejności FIFO / 1 w kolejności LIFO:\n");
    scanf("%d", &i);
    if(i == 0){
        for(i = 0; i < size; i++){
            printf("tab[%d] = %d\n", i, tab[i]);
        }
    }
        else if(i == 1){
            for(i = size-1; i >= 0; i--){
                printf("tab[%d] = %d\n", i, tab[i]);
            }
        }
    
    return 0;
}

